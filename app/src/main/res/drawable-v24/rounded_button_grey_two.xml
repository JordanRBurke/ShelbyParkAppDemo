<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#656673" />
            <gradient android:angle="-90" android:startColor="#505360" android:endColor="#878e9f"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#767c86" />
            <solid android:color="#525561"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#44454e" />
            <gradient android:angle="-90" android:startColor="#717386" android:endColor="#565b6b" />
        </shape>
    </item>
</selector>